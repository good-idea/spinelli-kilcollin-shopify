{
	"name": "spkrebuild",
	"version": "1.0.0",
	"description": "Spinelli Kilcollin shopify theme rebuild",
	"main": "index.js",
	"scripts": {
		"dev": "npm-run-all --parallel dev:js dev:css dev:theme",
		"dev:js": "webpack --env development",
		"dev:css": "postcss src/css/main.css -o assets/main.css --watch",
		"dev:theme": "theme open -e development -v && theme watch -e development",
		"build": "npm run build:css && npm run build:js",
		"build:js": "webpack -p --env production",
		"build:css": "postcss src/css/main.css -o assets/main.css",
		"sync-config": "theme download -e production --no-ignore config/settings_data.json && theme deploy -e development --no-ignore config/settings_data.json",
		"backstop:test": "backstop test",
		"backstop:approve": "backstop approve",
		"deploy": "theme upload -e production"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/good-idea/spk-shopify.git"
	},
	"author": "Joseph Thomas & J0die",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/good-idea/spk-shopify/issues"
	},
	"homepage": "https://github.com/good-idea/spk-shopify#readme",
	"dependencies": {
		"@vimeo/player": "^2.7.0",
		"axios": "^0.18.0",
		"jquery": "^3.3.1",
		"js-cookie": "^2.2.0",
		"npm-zepto": "^1.1.7",
		"photoswipe": "^4.1.3",
		"q": "^1.5.1",
		"ramda": "^0.26.1",
		"scroll-to": "0.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.3",
		"@babel/node": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.3.3",
		"@babel/plugin-proposal-export-default-from": "^7.2.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.3.2",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-runtime": "^7.2.0",
		"@babel/polyfill": "^7.2.5",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"backstopjs": "^3.8.8",
		"cssnano": "^4.1.10",
		"eslint": "^5.14.1",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.0.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-webpack": "^0.11.0",
		"eslint-plugin-flowtype": "^3.4.2",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jest": "^22.3.0",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-react": "^7.12.4",
		"event-stream": "^4.0.1",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^6.1.1",
		"postcss-cssnext": "^3.1.0",
		"postcss-extend-rule": "^2.0.0",
		"postcss-import": "^12.0.1",
		"postcss-math": "0.0.10",
		"postcss-mixins": "^6.2.1",
